{
  "manifest_version": 2,

  "name": "Minerva Analytics Plugin",
  "description": "Browse Productively",
  "version": "1.0",

  "chrome_url_overrides" : {
    "newtab": "popup.html"
  },

  "browser_action": {
   "default_icon": "icon.png",
   "default_popup": "popup.html"
  },
  
  "permissions": [
   "activeTab"
   ]
}